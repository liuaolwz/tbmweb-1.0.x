<?xml  version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">

    <bean id="scriptManager" class="com.tumbleweed.platform.trunk.base.ScriptManager">
        <property name="scriptDirPath" value="classpath:/script"/>
        <property name="localDirPath" value="classpath:/local"/>
    </bean>

    <bean id="commonWorkQueue"
          class="com.tumbleweed.platform.trunk.scheduler.core.impl.CommonWorkQueueImpl" destroy-method="onDestroy" />

</beans>